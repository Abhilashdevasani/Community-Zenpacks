<tal:block tal:define="
        objects python:[card for card in here.getDmdRoot('Devices').getMonitoredComponents() if card.id.startswith('cpqSm2Cntlr')];
        tableName string: iloboards;
        batch python:here.ZenTableManager.getBatch(tableName,objects);
        exportFields python:[('getParentDeviceName','Device'), 
                            ('name','Component'), 
                            ('systemId', 'System ID'),
                            ('hwVer', 'HW Ver'),
                            ('romRev', 'ROM Rev'),
                            ('serialNumber', 'Serial Number'),
                            ('ipaddress','IP Address'), 
                            ('macaddress','MAC Address')];">
<tal:block metal:use-macro="here/reportMacros/macros/exportableReport">
<tal:block metal:fill-slot="report">

<tal:block metal:use-macro="here/templates/macros/page1">
<tal:block metal:fill-slot="breadCrumbPane">
    <span metal:use-macro="here/miscmacros/macros/reportBreadCrumbsList"/>    
</tal:block>
<tal:block metal:fill-slot="contentPane">

<form method="POST" tal:attributes="action request/URL;"
                        tal:define="tabletitle string:iLO Boards;
                        showfilterbox python:True;
                        tblcolspan string:5">
<tal:block metal:use-macro="here/zenuimacros/macros/zentable">
<tal:block metal:fill-slot="zentablecontents">

    <tr>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                            tableName,'hostname','Device')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                            tableName,'name','Component')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                            tableName,'systemId','System ID')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                            tableName,'hwVer','HW Ver')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                            tableName,'romRev','ROM Rev')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                            tableName,'serialNumber','Serial Number')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                            tableName,'macaddress','IP Address')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                            tableName,'macaddress','MAC Address')"/>
    </tr>
    <tr tal:repeat="comp batch">
        <td class="tablevalues" 
          tal:content="structure comp/getDeviceLink"/>
        <td class="tablevalues">
            <a class="tablevalues" tal:attributes="href comp/getPrimaryUrlPath"
                tal:content="comp/name">smtp</a>
        </td>
        <td class="tablevalues" tal:content="comp/systemId"/>
        <td class="tablevalues" tal:content="comp/hwVer"/>
        <td class="tablevalues" tal:content="comp/romRev"/>
        <td class="tablevalues" tal:content="comp/serialNumber"/>
        <td class="tablevalues" tal:content="comp/ipaddress"/>
        <td class="tablevalues" tal:content="comp/macaddress"/>
    </tr>
    <tr>
        <td colspan="5" class="tableheader" align='center'>
        <form metal:use-macro="here/zenTableNavigation/macros/navtool"/>
        </td>
    </tr>

</tal:block>
</tal:block>
</form>

</tal:block>
</tal:block>

</tal:block>
</tal:block>
</tal:block>


    <tr>
      <td class="tableheader" align=left>Hardware Version</td>
      <td class="tablevalues" tal:content="here/hwVer">1</td>
      <td class="tableheader" align=left>ROM Version</td>
      <td class="tablevalues" tal:content="here/romRev">1</td>
    </tr>
    <tr>
      <td class="tableheader" align=left>MAC Address</td>
      <td class="tablevalues" tal:content="here/macaddress">00:11:22:AA:BB:CC:DD</td>
      <td class="tableheader" align=left>DNS Name</td>
      <td class="tablevalues" tal:content="here/dnsName">1</td>
    </tr>
    <tr>
      <td class="tableheader" align=left>IP Address</td>
      <td class="tablevalues" tal:content="here/ipaddress">1</td>
      <td class="tableheader" align=left>Subnet Mask</td>
      <td class="tablevalues" tal:content="here/subnetmask">1</td>
    </tr>
    <tr>
      <td class="tableheader" align=left>Serial Number</td>
      <td class="tablevalues" tal:content="here/serialNumber">1</td>
      <td class="tableheader" align=left>System ID</td>
      <td class="tablevalues" tal:content="here/systemId">1</td>
    </tr>
